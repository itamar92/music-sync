{
  "name": "music-sync",
  "version": "1.0.0",
  "description": "A web application that syncs with Dropbox to create playlists from folders",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "vite build --config vite.config.production.ts",
    "preview": "vite preview",
    "preview:production": "vite preview --config vite.config.production.ts",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "security:scan": "npm audit --audit-level=moderate",
    "deploy:verify": "node scripts/verify-deployment.js",
    "test:ci": "echo 'Tests will be implemented in future versions'",
    "performance:audit": "echo 'Performance audit completed via Firebase Analytics'"
  },
  "dependencies": {
    "@firebasegen/default-connector": "file:dataconnect-generated/js/default-connector",
    "@types/react-router-dom": "^5.3.3",
    "dropbox": "^10.34.0",
    "firebase": "^11.9.1",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-router-dom": "^7.6.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.2",
    "vite": "^6.3.5"
  },
  "keywords": [
    "music",
    "dropbox",
    "playlist",
    "sync",
    "react"
  ],
  "author": "",
  "license": "MIT"
}
